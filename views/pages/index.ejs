<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>PE Tracker</title>
</head>
<body>
    <h1>PE Tracker</h1>
    <% tasks.forEach(function (r) {%>
        <button id='<%= r.id %>'
            onclick='javascript:logTask(<%= r.id %>);'><%= r.name %>
        </button>
    <% }); %>
</body>
</html>